#include<bits/stdc++.h>
using namespace std;

struct Node
 {

int w, p;

};

Node arr []= { {15,15}, {20,40}, {18,27}};

 bool cmp (Node a, Node b)
 {
     return a.p * b. w > b.p * a.w;

 }
 int main ()
 {
     sort (&arr[0], &arr [2+1], cmp); for (int i=0; i<3; i++)
     cout << arr [i].w<< " "  << arr [i].p << endl;
     return 0;

}
